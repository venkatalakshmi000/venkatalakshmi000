n=int(input('enter no. of numbers:'))
x=list(map(int,input('enter the numbers:').split()))
m=int(input('enter the required number:'))
flag=0
for i in range(0,n):
    for j in range(1,n):
        if x[i]-x[j]==m:
            flag=1
            break
if flag==1:
    print('yes')
else:
    print('no')
