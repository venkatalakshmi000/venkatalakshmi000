n=int(input('enter no. of numbers in list:'))
x=list(map(int,input('enter the numbers:').split()))
s=int(input('enter number to search:'))
flag=0
for i in range(0,n):
    if x[i]==s:
        flag=1
        break
if flag==1:
    print('element found at index:',i)
else:
    print('element not found')
