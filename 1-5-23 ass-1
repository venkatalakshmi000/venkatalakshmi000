s={}
n=int(input('enter no of players:'))
for i in range(1,n+1):
    l=[]
    pno=int(input('enter player number:'))
    pname=input('enter player name:')
    l.append(pno)
    l.append(pname)
    s[pno]=l
print(s)
pno=int(input('enter player number:'))
print('pno:',s[pno][0])
print('pname:',s[pno][1])
